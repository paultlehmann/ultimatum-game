  },
  "devDependencies": {
    "@types/express": "^4.17.19",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.5",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
